/* WRITE */
.tblWrap table.tbl_write tr th{ width:200px;background:#FBFBFB;border-bottom:1px solid #E6E6E6;height:57px;font-size:14px;text-align:center;padding:0 15px; }
.tblWrap table.tbl_write tr td{ /*width:380px;*/padding:0 0 0 10px;border-bottom:1px solid #E6E6E6 }
.tblWrap table.tbl_write tr td.inFileCase{ padding:5px 0 5px 10px }
.tblWrap table.tbl_write tr td select{ float:left;width:304px;height:40px;font-size:14px;border:1px solid #D0D0D0 }
.tblWrap table.tbl_write input[type=text].form_write{ float:left;width:884px;height:40px;border:1px solid #D0D0D0;font-size:14px }
.tblWrap table.tbl_write tr td.textarea{ height:415px }
.tblWrap table.tbl_write tr td.textarea .write_textarea{ float:left;width:884px;height:350px;border:1px solid #D0D0D0;font-size:14px;padding:10px }
.fileBox{ float:left;width:885px;height:40px;margin:0 0 5px 0 }
.fileBox input[type=file]{ float:left;width:832px;height:40px;font-size:13px;border:1px solid #D0D0D0;padding:8px 0 0 6px;background:#FFF }
.fileBox .btnFileDel{ float:left;width:48px;height:40px;margin:0 0 0 5px;text-align:center;font-weight:bold;font-size:13px;padding:0;border:1px solid #D0D0D0;background:#FFF }
.fileBox .btnFileDel:hover, .fileBox .btnFileDel:focus{ border:1px solid #5D7CFE;color:#4E6DF1;cursor:pointer }
.fileBox .memo{ float:left;font-size:12px;color:#585757 } 
.filename{ float:left;width:100%;/*border-bottom:1px solid #EEE;*/font-size:13px;margin:9px 0 0 0;padding:0 0 8px 18px;position:relative }
.filename:before{ position:absolute;content:'';width:4px;height:2px;background:#FF0000;left:5px;top:8px }
.filename a{ color:#323232 }
.filename a:hover, .filename a:focus{ text-decoration:underline }
.filename:last-child{ border-bottom:0 }
.savedFileList{ float:left;width:885px }
.del_savedFile{ float:left;width:54px;height:20px;font-size:12px;font-weight:bold;border:1px solid #D0D0D0;background:#FFF;color:#747474;padding:0 0 0 20px;margin:0 0 0 10px;position:relative }
.del_savedFile:before{ position:absolute;content:'';left:6px;top:3px;width:11px;height:12px;background:url(../images/yeartax_help/img_trash.png) no-repeat 0 0 }
.del_savedFile:hover, .del_savedFile:focus{ border:1px solid #888585;color:#333333;cursor:pointer }
.del_savedFile:hover:before, .del_savedFile:focus:before{ background:url(../images/yeartax_help/img_trash.png) no-repeat 0 -35px }

/* REPLY */
.tblWrap table.tbl_reply tr th{ width:200px;height:57px;font-size:16px;text-align:center;padding:0 15px; }
.tblWrap table.tbl_reply tr td{ width:380px }
.tblWrap table.tbl_reply tr.view th{ width:200px;height:34px;font-size:14px;text-align:center;padding:0 15px; }
.tblWrap table.tbl_reply tr.view td{ width:380px;font-size:14px;border-bottom:1px solid #E8E8E8 }
.tblWrap table.tbl_reply tr td select{ float:left;width:304px;height:40px;font-size:14px;border:1px solid #D0D0D0 }
.tblWrap table.tbl_reply input[type=text].form_write{ float:left;width:884px;height:40px;border:1px solid #D0D0D0;font-size:14px }
.tblWrap table.tbl_reply tr td.textarea.question{ height:200px }
.tblWrap table.tbl_reply tr td.textarea.answer{ height:364px }
.tblWrap table.tbl_reply tr td.textarea .write_textarea{ float:left;width:884px;border:1px solid #D0D0D0;font-size:14px;padding:10px }
.tblWrap table.tbl_reply tr td.textarea.question .write_textarea{ height:200px }
.tblWrap table.tbl_reply tr td.textarea.answer .write_textarea{ height:350px }

/* view */
.tblWrap table.tbl_view tr th{ width:200px;height:34px;background:#FBFBFB;font-size:14px;text-align:center;padding:0 15px;border-top: 1px solid #e8e8e8 }
.tblWrap table.tbl_view tr td{ font-size:14px;border-top: 1px solid #e8e8e8;border-bottom: 1px solid #e8e8e8;padding:0 0 0 10px }
.tblWrap table.tbl_view tr td.title{  height:86px;font-size:22px;font-weight:bold;padding:0 0 0 110px; }
.tblWrap table.tbl_view tr td.textarea{height:364px;min-height:364px;padding:10px;vertical-align:top }
.tblWrap table.tbl_view tr th.th_A{ background:#f7fef1 }
.tblWrap table.tbl_view tr .th_a_bg{background:#f7fef1 }
.tblWrap table.tbl_view.reply tr td.title{ background:url(../images/yeartax_help/icon_reply_g.png) no-repeat 55px 25px;border-bottom:1px solid #a2f091 }
.tblWrap table.tbl_view.question tr td.title{ background:url(../images/yeartax_help/icon_question.png) no-repeat 55px 35px }
.tblWrap .tbl_view {border-bottom:1px solid #e8e8e8 }
.tblWrap table.tbl_view.reply tr th.th_A,
.tblWrap table.tbl_view.reply tr td.textarea{ border-bottom:1px solid #a2f091 }
.tblWrap table.tbl_view.reply .th_a_line{border-bottom:1px solid #a2f091; }
.reply {border-bottom:1px solid #a2f091; }

/* buttons */
.tblBtnArea{ float:left;width:100%;margin:40px 0 0 0 }

.tblBtnArea.writeForm{ width:100%;margin:0 0 0 0; /*padding:20px 0 30px 201px*/ position:relative}
.tblBtnArea.writeForm .btnWriteBasic{ float:left;width:80px;height:35px;font-size:14px;font-weight:bold;padding:8px 0 0 20px;border-radius:5px;text-align:center;background:#EFF0F2;cursor:pointer }
.tblBtnArea.writeForm .btnWriteBasic:hover, .tblBtnArea.writeForm .btnWriteBasic:focus{ opacity:0.8; }
.tblBtnArea.writeForm .btnWriteBasic:first-child{ margin:0 4px 0 0 }
.tblBtnArea.writeForm .saveBtn{position:absolute;top:0;right:280px;background:url(../images/yeartax_help/icon1_save.png) no-repeat 18% 50% #508CF7; color:#fff;}
.tblBtnArea.writeForm .cancelBtn{position:absolute;right:200px;background:url(../images/yeartax_help/icon2_cancel.png) no-repeat 18% 50% #cacaca;}
.tblBtnArea.writeForm .saveBtn2{position:absolute;top:-50px;right:20px;background:url(../images/yeartax_help/icon1_save.png) no-repeat 18% 50% #14C753; color:#fff;}
.tblBtnArea.writeForm .cancelBtn2{position:absolute;right:200px;background:url(../images/yeartax_help/icon2_cancel.png) no-repeat 18% 50% #cacaca;}
.nomalbtnWrap {clear:both;width:100%; height:50px;}
.nomalbtnWrap .nomalBtn{width:350px; height:50px; float:left; border-radius:5px; text-align:center;margin:3% 2%;}

.btBasic_search{width:120px;height:35px; position: absolute; right:230px;top:40px;text-align:center; line-height:32px;border-radius:3px;border:2px solid #508CF7;box-sizing:borer-box;color:#1746cf;font-weight:bold;}
.btBasic_search:hover, .btBasic_search:focus{ transition:0.2s; cursor:pointer; background:#eee; color:#333; border:2px solid #ccc;}
.btnBasic1{width:120px;height:35px; position: absolute;right: 360px;top: 40px;font-size:14px;font-weight:bold;padding:7px 0 0 13px;margin: 0 10px 15px 10px;border-radius:3px;text-align:center;color:#fff;border:2px solid #508CF7;background:url(../images/yeartax_help/icon_write_w.png) no-repeat 17px 11px #508CF7;cursor:pointer }
.btnBasic{width:120px;height:35px;font-size:14px;font-weight:bold;padding:7px 0 0 13px;margin: 0 10px 15px 10px;border-radius:3px;text-align:center;color:#333;background:url(../images/yeartax_help/icon_write_w.png) no-repeat 17px 11px #508CF7;cursor:pointer }
.btnBasic1:hover, .btnBasic1:focus{background:url(../images/yeartax_help/icon_write.png) no-repeat 17px 11px #fff; border:2px solid #508CF7;box-sizing:borer-box;color:#1746cf; }
.btnBasic:hover, .btnBasic:focus{  color:#333; transition:0.2s;}	
.btnBasic.list{ float:left;padding:7px 0 0 0;background:#EFF0F2 }	
.btnBasic.edit{ float:right;padding:7px 0 0 0;background:#EFF0F2 }	
.btnBasic.del{ float:right;padding:7px 0 0 0;background:#efefef;margin:0 0 0 5px }
.btnBasic.edit{ float:right;padding:7px 0 0 0;background:#14c753;margin:0 0 0 5px }

.btnBasic.reply{ float:right;padding:7px 0 0 0;background:#EFF0F2;margin:0 0 0 5px }	
.btnBasic.list:hover, .btnBasic.list:focus,
.btnBasic.edit:hover, .btnBasic.edit:focus,
.btnBasic.del:hover, .btnBasic.del:focus,
