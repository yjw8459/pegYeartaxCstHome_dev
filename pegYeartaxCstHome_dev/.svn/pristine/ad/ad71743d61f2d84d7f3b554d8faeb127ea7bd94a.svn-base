.tblBtnTop{width: 100%;position: relative;}

.searchIcon {display:inline-block; width:30px; height:30px; margin-top:2%; cursor:pointer; background:url(../images/yeartax_help/icon.png)no-repeat; background-size:cover;}
.searchIcon:hover{transition:0.5s; transform:rotate(360deg); }

/* FAQ LIST */
.faqWrap{ float:left;width:100% }
.faq_item{ float:left;width:100%;border-bottom:1px solid #F1F1F1 }
.faq_item .btn_faq{ float:left;width:100%;padding:17px 30px 19px 0 }
.faq_item .btn_faq .txt_cat{ float:left;padding-right:6px;color:#2282F9;font-size:16px }
.faq_item .btn_faq .faq_TitleBox{ float:left;width:988px;font-size:16px;color:#383D48 }
.faq_item .btn_faq .bg_cs_open{ float:right;width:11px;height:6px;background-image:url(../images/yeartax_help/open_cs_in_faq.png);background-repeat:no-repeat;background-position:0 0;margin:12px 10px 0 0 }
.faq_item .btn_faq:hover .faq_TitleBox, .faq_item .btn_faq:focus .faq_TitleBox{ color:#2282F9;cursor:pointer;text-decoration:underline }
.faq_item .btn_faq:hover .bg_cs_open, .faq_item .btn_faq:focus .bg_cs_open{ background-position:0 -10px;cursor:pointer }

/* display:none; */
.faq_item.on .faq_view{ display:block }
.faq_item.on .btn_faq{ display:none }
.faq_item .faq_view{ display:none;float:left;width:100%;padding-bottom:60px;border-top:1px solid #C3C4C8;border-bottom:1px solid #C3C4C8;margin-bottom:-1px;margin-top:-1px }
.faq_item .faq_view .tit_faq{ float:left;width:1064px;padding:47px 0 48px 0;margin:0 0 35px 0;font-weight:normal;font-size:28px;color:#2282F9;border-bottom:1px solid #E6E9EC }
.faq_item .faq_view .desc_faq{ float:left;width:90%;font-size:16px;line-height: 1.875 }
.faq_item .faq_view .bg_cs_close{ float:right;width:21px;height:20px;margin:60px 33px 0 0;background-image:url(../images/yeartax_help/btn_close_in_faq.png);background-repeat:no-repeat;background-position:5px 5px }
.faq_item .faq_view .bg_cs_close:hover, .faq_item .faq_view .bg_cs_close:focus{ background-position:5px -10px;cursor:pointer }
.faq_item .faq_view .btnWrap{ float:left;width:100%;padding:0 40px 0 0 }
.faq_item .faq_view .btnWrap button{ float:right;border:1px solid #DDD;background:#F7F6F6;font-size:13px;padding:4px 8px 3px 8px;margin:0 0 0 5px;cursor:pointer }
.faq_item .faq_view .btnWrap button:hover, .faq_item .faq_view .btnWrap button:focus{ background:#E2E2E2 }

/* PAGINATE */
.page_Container{ float:left;width:100%;text-align:center; margin-top:30px; }
.paginate{display:inline-block;position:relative;height:30px;text-align:center;margin:20px 0}
.paginate a{ float:left;width:26px;height:24px;text-align:center;line-height:22px;background-image:url(../images/yeartax_help/paginate.png);background-repeat:no-repeat;font-size:12px;font-weight:bold }
.paginate .prevEnd, .paginate .num, .paginate .prevEnd:focus, .paginate .prevEnd:hover,
.paginate .num_current, .paginate .num:hover, .paginate .num:focus,
.paginate .num_current:hover, .paginate .num_current:focus { margin-right:4px }
.paginate .prev, .paginate .prev:focus, .paginate .prev:hover{ margin-right:10px }
.paginate .next, .paginate .next:focus, .paginate .next:hover { margin:0 4px 0 6px }
.paginate .prevEnd{ background-position:0 0 }
.paginate .prev{ background-position:-30px -0 }
.paginate .prevEnd:focus, .paginate .prevEnd:hover{ background-position:0px -25px }
.paginate .prev:focus, .paginate .prev:hover{ background-position:-30px -25px }
.paginate .num{ color:#A3A3A3;background-position:-89px 0 }
.paginate .num_current{ color:#FFF;background-position:-59px 0 }
.paginate .num:hover, .paginate .num:focus{ color:#A3A3A3;background-position:-89px -25px }
.paginate .num_current:hover, .paginate .num_current:focus{ color:#FFF;background-position:-59px -25px }
.paginate .next{ background-position:-119px 0 }
.paginate .nextEnd{ background-position:-149px 0 }
.paginate .next:focus, .paginate .next:hover{ background-position:-119px -25px }
.paginate .nextEnd:focus, .paginate .nextEnd:hover{ background-position:-149px -25px }


