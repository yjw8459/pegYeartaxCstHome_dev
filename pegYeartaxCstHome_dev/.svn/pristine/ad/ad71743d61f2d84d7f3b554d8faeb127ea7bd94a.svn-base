////////////// */
.mainWrap{ float:left;width:100%;background:url(../images/yeartax_help/div_line.png) repeat-y 209px 0 #FFF;position:relative }
.mainWrap .header{ position:absolute;width:100%;height:96px;padding:28px 0 0 0;background:url(../images/yeartax_help/hp_header_logo.png) no-repeat 30px 34px #F5F5F5;border-bottom:1px solid #E0E0E0 }
