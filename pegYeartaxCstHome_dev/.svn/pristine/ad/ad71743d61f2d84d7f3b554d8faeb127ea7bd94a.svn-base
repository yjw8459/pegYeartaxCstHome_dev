@charset "utf-8";
@import "reset.css";

/* /////////////////////////////////////////
	=basic style
///////////////////////////////////////// */

*, *::before, *::after{ -webkit-box-sizing:border-box;-moz-box-sizing:border-box;-o-box-sizing:border-box;-ms-box-sizing:border-box;box-sizing:border-box }
body, table, input, select, a, div, textarea, li{ font-family:'MalgunGothic', sans-serif }
p, table{ margin:0 }
input[type=text].date,
input[type=text].datepicker,
input[type=text].hasDatepicker { text-align:center;padding-left:0;padding-right:0 }
body{ margin:0;padding:0;position:relative }
body.yeartaxHelp_main{ overflow:hidden }
body.yeartaxHelp_login{ background:url(../images/yeartax_help/white_bk_700px.png) repeat-y center top #F5F5F5 }

/* Chrome, Safari용 스크롤 바 */
::-webkit-scrollbar { width:17px; height:17px }
::-webkit-scrollbar-button:start:decrement 	{ display:block;height:34px;background:url(../images/scrollbar/scrollbar_start.png) no-repeat left top }
::-webkit-scrollbar-button:end:increment  	{ display:block;height:34px;background:url(../images/scrollbar/scrollbar_end.png) no-repeat left top }
::-webkit-scrollbar-button:start:decrement:horizontal	{ display:block;width:34px;height:17px;background:url(../images/scrollbar/scrollbar_start_horizontal.png) no-repeat left top  }
::-webkit-scrollbar-button:end:increment:horizontal	{ display:block;width:34px;height:17px;background:url(../images/scrollbar/scrollbar_end_horizontal.png) no-repeat right top }
::-webkit-scrollbar-track { background:#F0F0F0 }
::-webkit-scrollbar-track:horizontal{ border-bottom:0}
::-webkit-scrollbar-thumb { height:49px;width:50px;background:#CDCDCD }
::-webkit-scrollbar-corner{ background:#F0F0F0 }

table{ font-size:13px }
input[type=password],
input[type=text]{ float:left;padding:0 0 0 7px;margin-right: 10px;height:40px;font-size:13px;border:1px solid #D0D0D0;border-radius:0;/*border-bottom:1px solid #D0D0D0*/}
input[type=number]{ float:left;padding:0 0 0 7px;height:40px;font-size:13px;border:0;border-radius:0;border-bottom:1px solid #E6E6E6 }
select{ float:left;width:100%;font-size:13px;height:25px;border:1px solid #E6E6E6 }

/* /////////////////////////////////////////
	=common style
///////////////////////////////////////// */

/* //////////////
  = 로그인
////////////// */
.windowWrap{ float:left;width:100%;text-align:center }
.contentWrap{ display:inline-block;width:700px;background:url(../images/yeartax_help/hp_login_logo.png) no-repeat center 82px }
.loginBox{ float:left;width:478px;margin:290px 0 0 110px;border:1px solid #E5E5E5;padding:130px 0 68px 70px;background:url(../images/yeartax_help/login_title.png) no-repeat 70px 57px #FFF }
.loginBox input[type=text].hp_id{ width:338px;height:51px;border-bottom:2px solid #EBEBEB;font-weight:bold;font-size:14px }
.loginBox input[type=password].hp_pw{ width:338px;height:51px;border-bottom:2px solid #EBEBEB;font-weight:bold }
.loginBox .saveidBox{ float:right;margin:5px 74px 0 0 }
.loginBox .saveidBox input[type=checkbox]{ float:left;width:14px;height:14px;margin:3px 6px 3px 4px;cursor:pointer }
.loginBox .saveidBox label{ float:left;font-size:13px;font-weight:bold;cursor:pointer }
input:-internal-autofill-selected{ background:#FFF!important }
.hp_btn{ float:left;width:167px;height:50px;padding:14px 0 0 0;border-radius:3px;font-weight:bold;text-align:center;font-size:14px;letter-spacing:-1px;cursor:pointer }
.hp_btn.login{ width:334px;background:#FEE500;margin:34px 0 0 0 }
.hp_btn.pwReset{ background:#F6F6F6;margin:0 4px 0 0 }
.hp_btn.joinRequest{ background:#F6F6F6 }
.hp_btn.login:hover, .hp_btn.login:focus{ background:#FED700 }
.hp_btn.pwReset:hover, .hp_btn.pwReset:focus,
.hp_btn.joinRequest:hover, .hp_btn.joinRequest:focus{ background:#E7E7E7 }
.btw{ float:left;width:338px;height:48px;background:url(../images/yeartax_help/btw.png) no-repeat }

/* //////////////
  = 비밀번호 초기화 
////////////// */
.pwResetBox{ float:left;width:478px;margin:290px 0 0 110px;border:1px solid #E5E5E5;padding:130px 0 68px 70px;background:url(../images/yeartax_help/pwreset_title.png) no-repeat 70px 57px #FFF }
.pwResetBox input[type=text].hp_id, .pwResetBox input[type=text].hp_name,
.pwResetBox input[type=text].hp_email{ width:338px;height:51px;border-bottom:2px solid #EBEBEB;font-weight:bold }



/* //////////////
  = 로딩 이미지
////////////// */
.loading{ display:none;width:100%;height:100%;position:absolute;top:0;left:0;z-index:999;background:url(../images/yeartax_help/opacity_50.png)  }
.loading:before { content:'';position:absolute;width:150px;height:150px;border:1px solid #88B7CC;background:url(../images/yeartax_help/loader_backinout.gif) no-repeat;top:35%;left:45% }



